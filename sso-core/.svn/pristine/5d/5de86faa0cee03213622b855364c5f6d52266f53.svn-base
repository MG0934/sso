public enum JWTCheckEnum {
	
	EXPORT(301,"超时",false),
	RELET(201,"续租",false),
	NORMAL(101,"正常",true);
	
	private Integer code;
	
	private String desc;
	
	private boolean bv;
	
	JWTCheckEnum(Integer code,String desc,boolean bv) {
		
		this.code = code;
		this.desc = desc;
		this.bv = bv;
	}

	public Integer getCode() {
		return code;
	}

	public String getDesc() {
		return desc;
	}

	public boolean isBv() {
		return bv;
	}
	
}
